<%- model_class = Subscription -%>
<div class="page-header">
  <h1><%= @subscription.email %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:id) %>:</strong></dt>
  <dd><%= @subscription.id %>&nbsp;</dd>
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @subscription.name %>&nbsp;</dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @subscription.status %>&nbsp;</dd>
  <dt><strong><%= model_class.human_attribute_name(:created_at) %>:</strong></dt>
  <dd><%= @subscription.created_at %>&nbsp;</dd>
  <dt><strong><%= model_class.human_attribute_name(:updated_at) %>:</strong></dt>
  <dd><%= @subscription.updated_at %>&nbsp;</dd>
</dl>

<div class="form-actions">
<%= link_to t('.back', :default => t("helpers.links.back")),
              admin_subscriptions_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_admin_subscription_path(@subscription), :class => 'btn btn-default' %>
</div>